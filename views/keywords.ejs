<%- include('partials/header.ejs') %>
<style>
body {
    background-repeat: repeat;
    background-size: auto;
}
</style>
<div id="favContainer">
   <a class="link" href="/"> Back to Search </a>
   <h1>Keyword Counts</h1>
   <% 
      rows.forEach(function(row, i){ %>
       
        <a class="keywordLink" href="#"><%= row["keyword"] %></a>
    
        <%- ((i+1) % 6 == 0)?'<br><br>':'' %>
    
    <% });%>
    
    <div id="keywords"></div>
    
    <input type="hidden" id="keywordSelected">
</div>
<%- include('partials/footer.ejs') %>